select a.payee_id%8, a.* from (
    select d.id as did,c.* from incomenew_bcp_00.v_collection_transaction_0000 c,incomenew_bcp_00.v_bill_0000 d where c.business_transaction_no = d.business_transaction_no and c.policy_no like 'T%' union all
    select d.id as did,c.* from incomenew_bcp_00.v_collection_transaction_0001 c,incomenew_bcp_00.v_bill_0001 d where c.business_transaction_no = d.business_transaction_no and c.policy_no like 'T%' union all
    select d.id as did,c.* from incomenew_bcp_00.v_collection_transaction_0002 c,incomenew_bcp_00.v_bill_0002 d where c.business_transaction_no = d.business_transaction_no and c.policy_no like 'T%' union all
    select d.id as did,c.* from incomenew_bcp_00.v_collection_transaction_0003 c,incomenew_bcp_00.v_bill_0003 d where c.business_transaction_no = d.business_transaction_no and c.policy_no like 'T%' union all
    select d.id as did,c.* from incomenew_bcp_00.v_collection_transaction_0004 c,incomenew_bcp_00.v_bill_0004 d where c.business_transaction_no = d.business_transaction_no and c.policy_no like 'T%' union all
    select d.id as did,c.* from incomenew_bcp_00.v_collection_transaction_0005 c,incomenew_bcp_00.v_bill_0005 d where c.business_transaction_no = d.business_transaction_no and c.policy_no like 'T%' union all
    select d.id as did,c.* from incomenew_bcp_00.v_collection_transaction_0006 c,incomenew_bcp_00.v_bill_0006 d where c.business_transaction_no = d.business_transaction_no and c.policy_no like 'T%' union all
